	<LinearLayout 
         android:layout_width="fill_parent"
         android:layout_height="1dp"
         android:background="#ebe9e4"
         android:orientation="vertical"
         ></LinearLayout>     
  	<RelativeLayout 
        android:id="@+id/postcardFrame"
        android:layout_width="fill_parent"
        android:layout_height="46dp"
        android:padding="0dp"
        android:visibility="gone"
        >
        <TextView
        	android:layout_width="wrap_content"
        	android:layout_height="wrap_content"
        	android:layout_centerVertical="true"
        	android:layout_marginLeft="12dp"
        	android:text="Postcard 수신 알림"
        	android:textSize="16dp"
        	android:textStyle="bold"/>
        <Button 
            android:id="@+id/postcardBtn"
            android:layout_width="19dp"
            android:layout_height="16dp"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:layout_marginRight="10dp"
            android:background="@drawable/selector_check_settings"/>
        
    </RelativeLayout>
	<LinearLayout 
         android:layout_width="fill_parent"
         android:layout_height="1dp"
         android:background="#ebe9e4"
         android:orientation="vertical"
         ></LinearLayout>         
	<RelativeLayout 
        android:id="@+id/myClassFrame"
        android:layout_width="fill_parent"
        android:layout_height="46dp"
        android:padding="0dp"
        android:visibility="gone"
        >
        <TextView
        	android:layout_width="wrap_content"
        	android:layout_height="wrap_content"
        	android:layout_centerVertical="true"
        	android:layout_marginLeft="12dp"
        	android:text="내가 참여한 클래스 소식"
        	android:textSize="16dp"
        	android:textStyle="bold"/>
        <Button 
            android:id="@+id/myClassBtn"
            android:layout_width="19dp"
            android:layout_height="16dp"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:layout_marginRight="10dp"
            android:background="@drawable/selector_check_settings"/>
        
    </RelativeLayout>
	<LinearLayout 
         android:layout_width="fill_parent"
         android:layout_height="1dp"
         android:background="#ebe9e4"
         android:orientation="vertical"
         ></LinearLayout>      
	<RelativeLayout 
        android:id="@+id/classFrame"
        android:layout_width="fill_parent"
        android:layout_height="46dp"
        android:padding="0dp"
        android:visibility="gone"
        >
        <TextView
        	android:layout_width="wrap_content"
        	android:layout_height="wrap_content"
        	android:layout_centerVertical="true"
        	android:layout_marginLeft="12dp"
        	android:text="아트 클래스 오픈 소식"
        	android:textSize="16dp"
        	android:textStyle="bold"/>
        <Button 
            android:id="@+id/classBtn"
            android:layout_width="19dp"
            android:layout_height="16dp"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:layout_marginRight="10dp"
            android:background="@drawable/selector_check_settings"/>
        
    </RelativeLayout>
	<LinearLayout 
         android:layout_width="fill_parent"
         android:layout_height="1dp"
         android:background="#ebe9e4"
         android:orientation="vertical"
         ></LinearLayout>         
	<RelativeLayout 
