	<LinearLayout 
         android:layout_width="fill_parent"
         android:layout_height="1dp"
         android:background="#ebe9e4"
         android:orientation="vertical"
         ></LinearLayout> 
 	<TextView 
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="20dp"
        android:text="@string/setting_lb_notification_title_02"
        android:textSize="16dp"
        android:textStyle="bold"
        android:textColor="#222222"
        />
	<LinearLayout 
         android:layout_width="fill_parent"
         android:layout_height="1dp"
         android:background="#ebe9e4"
         android:orientation="vertical"
         android:layout_marginTop="12dp"
         ></LinearLayout> 
	<RelativeLayout 
        android:id="@+id/pushFrame"
        android:layout_width="fill_parent"
        android:layout_height="46dp"
        android:padding="0dp"
        >
        <TextView
        	android:layout_width="wrap_content"
        	android:layout_height="wrap_content"
        	android:layout_centerVertical="true"
        	android:layout_marginLeft="12dp"
        	android:text="@string/setting_lb_notification_title_02"
        	android:textSize="16dp"
        	android:textStyle="bold"/>
        <Button 
            android:id="@+id/pushBtn"
            android:layout_width="19dp"
            android:layout_height="16dp"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:layout_marginRight="10dp"
            android:background="@drawable/selector_check_settings"/>
        
    </RelativeLayout>
	<LinearLayout 
         android:layout_width="fill_parent"
         android:layout_height="1dp"
         android:background="#ebe9e4"
         android:orientation="vertical"
         ></LinearLayout>   
 	<TextView 
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="12dp"
        android:text="@string/setting_lb_notification_using_app"
        android:textSize="14dp"
        android:textStyle="bold"
        android:textColor="#888888"
        />                                                 
</LinearLayout>
