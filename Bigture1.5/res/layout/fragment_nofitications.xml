<?xml version="1.0" encoding="utf-8"?>
<LinearLayout 
    xmlns:android="http://schemas.android.com/apk/res/android"
   	xmlns:ptr="http://schemas.android.com/apk/res-auto"
    android:layout_width="456dp"
    android:layout_height="wrap_content"
    android:orientation="vertical"
    android:padding="0dp">
   
    <TextView 
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="30dp"
        android:text="@string/setting_lb_notification_title_01"
        android:textSize="16dp"
        android:textStyle="bold"
        android:textColor="#222222"
        />
	<LinearLayout 
         android:layout_width="fill_parent"
         android:layout_height="1dp"
         android:background="#ebe9e4"
         android:orientation="vertical"
         android:layout_marginTop="12dp"
         ></LinearLayout>      
    <RelativeLayout 
        android:id="@+id/talkFrame"
        android:layout_width="fill_parent"
        android:layout_height="46dp"
        android:padding="0dp"
        >
        <TextView
        	android:layout_width="wrap_content"
        	android:layout_height="wrap_content"
        	android:layout_centerVertical="true"
        	android:layout_marginLeft="12dp"
        	android:text="@string/setting_lb_notification_reply_myartwork"
        	android:textSize="16dp"
        	android:textStyle="bold"/>
        <Button 
            android:id="@+id/talkBtn"
            android:layout_width="19dp"
            android:layout_height="16dp"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:layout_marginRight="10dp"
            android:background="@drawable/selector_check_settings"/>
        
    </RelativeLayout>
	<LinearLayout 
         android:layout_width="fill_parent"
         android:layout_height="1dp"
         android:background="#ebe9e4"
         android:orientation="vertical"
         ></LinearLayout>   
	<RelativeLayout 
        android:id="@+id/likeFrame"
        android:layout_width="fill_parent"
        android:layout_height="46dp"
        android:padding="0dp"
        >
        <TextView
        	android:layout_width="wrap_content"
        	android:layout_height="wrap_content"
        	android:layout_centerVertical="true"
        	android:layout_marginLeft="12dp"
        	android:text="@string/setting_lb_notification_like"
        	android:textSize="16dp"
        	android:textStyle="bold"/>
        <Button 
            android:id="@+id/likeBtn"
            android:layout_width="19dp"
            android:layout_height="16dp"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:layout_marginRight="10dp"
            android:background="@drawable/selector_check_settings"/>
        
    </RelativeLayout>
	<LinearLayout 
         android:layout_width="fill_parent"
         android:layout_height="1dp"
         android:background="#ebe9e4"
         android:orientation="vertical"
         ></LinearLayout>  
	<RelativeLayout 
        android:id="@+id/pickFrame"
        android:layout_width="fill_parent"
        android:layout_height="46dp"
        android:padding="0dp"
        android:visibility="gone"
        >
        <TextView
        	android:layout_width="wrap_content"
        	android:layout_height="wrap_content"
        	android:layout_centerVertical="true"
        	android:layout_marginLeft="12dp"
        	android:text="@string/setting_lb_notification_picme"
        	android:textSize="16dp"
        	android:textStyle="bold"/>
        <Button 
            android:id="@+id/pickBtn"
            android:layout_width="19dp"
            android:layout_height="16dp"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:layout_marginRight="10dp"
            android:background="@drawable/selector_check_settings"/>
        
    </RelativeLayout>
	<LinearLayout 
         android:layout_width="fill_parent"
         android:layout_height="1dp"
         android:background="#ebe9e4"
         android:orientation="vertical"
         ></LinearLayout>  
     <RelativeLayout 
        android:id="@+id/contestFrame"
        android:layout_width="fill_parent"
        android:layout_height="46dp"
        android:padding="0dp"
        >
        <TextView
        	android:layout_width="wrap_content"
        	android:layout_height="wrap_content"
        	android:layout_centerVertical="true"
        	android:layout_marginLeft="12dp"
        	android:text="@string/setting_lb_notification_contest"
        	android:textSize="16dp"
        	android:textStyle="bold"/>
        <Button 
            android:id="@+id/contestBtn"
            android:layout_width="19dp"
            android:layout_height="16dp"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:layout_marginRight="10dp"
            android:background="@drawable/selector_check_settings"/>
        
